#include "common.h"

struct 	v2p
{
 	half2 	tc0: 		TEXCOORD0;	// base
 	half2 	tc1: 		TEXCOORD1;	// lmap
  	half4	c0:		COLOR0;		// sun
};

//////////////////////////////////////////////////////////////////////////////////////////
// Pixel
half4 	main	( v2p I )	: COLOR
{
	half4	t_base 	= tex2D	(s_base,I.tc0);
	t_base = pow(t_base, 2.2f);
	
	t_base.a *=  I.c0.a;

	// out
	return  t_base;
}
